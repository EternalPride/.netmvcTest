
@{
    ViewBag.Title = "AChEdit";
}

<h2>成绩编辑</h2>
<form method="post">
    <table>
        <tr>
            <td>
                Id
            </td>
            <td>
                <input id="Text1" type="text" name="AID" value=@ViewData["ID"]  />
            </td>
        </tr>
        <tr>
            <td>
                名字
            </td>
            <td>
                <input id="Text1" type="text" name="ANmae" value=@ViewBag.Name  />
            </td>
        </tr>
        <tr>
            <td>
                成绩
            </td>
            <td>
                <input id="Text1" type="text" name="Sem" value=@ViewBag.Sem  />
            </td>
        </tr>
        <tr>
            <td>
                类型
            </td>
            <td>
                <select id="Select1" name="Wotype">
                    <option selected="selected" value="value">启用</option>
                    <option>禁言</option>
                </select>
            </td>
        </tr>
        <tr>
            <td>
                成绩ID
            </td>
            <td>
                <input id="Text1" type="text" name="XHID" value=@ViewBag.XHID />
            </td>
        <tr>
            <td>
                <input id="Submit1" type="submit" value="添加" />
            </td>
            <td></td>
        </tr>
        </tr>
    </table>

</form>

